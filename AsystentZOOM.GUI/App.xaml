<Application x:Class="AsystentZOOM.GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="PanelWindow.xaml">
    <Application.Resources>
        <Image x:Key="Error"       Source="Images/icons8-add-property-32.png"/>
        <Image x:Key="Question"    Source="Images/icons8-alarm-clock-32.png"/>
        <Image x:Key="Warning"     Source="Images/icons8-all-out-32.png"/>
        <Image x:Key="Information" Source="Images/icons8-apps-tab-32.png"/>
        <Image x:Key="Ok"          Source="Images/icons8-below-32.png"/>
        <Image x:Key="Cancel"      Source="Images/icons8-cloud-sync-32.png"/>
    </Application.Resources>
</Application>
